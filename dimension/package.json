{
  "name": "space-stone",
  "version": "1.0.1",
  "description": "Personal portal to all other locations",
  "author": "AC De Leon <aadeleon@sdsu.edu>",
  "homepage": "https://acfromspace.github.io",
  "keywords": [
    "gatsby",
    "react.js"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/acfromspace/github.io"
  },
  "scripts": {
    "_comment": "To run these scripts use npm run SCRIPT_NAME, to exit develop process, CTRL + C",
    "dev": "gatsby develop",
    "lint": "./node_modules/.bin/eslint --ext .js,.jsx --ignore-pattern public .",
    "test": "echo \"Error: no test specified\" && exit 1",
    "develop": "gatsby develop",
    "build": "gatsby build",
    "deploy": "gatsby build && gh-pages -d public -b master",
    "fix-semi": "eslint --quiet --ignore-pattern node_modules --ignore-pattern public --parser babel-eslint --no-eslintrc --rule '{\"semi\": [2, \"never\"], \"no-extra-semi\": [2]}' --fix gatsby-node.js"
  },
  "dependencies": {
    "friendly-errors-webpack-plugin": "^1.7.0",
    "gatsby": "^1.9.277",
    "gatsby-image": "^1.0.55",
    "gatsby-link": "^1.6.46",
    "gatsby-plugin-google-analytics": "^1.0.31",
    "gatsby-plugin-react-helmet": "^2.0.11",
    "gatsby-plugin-sass": "^1.0.26",
    "gatsby-plugin-sharp": "^1.6.48",
    "gatsby-remark-copy-linked-files": "^1.5.37",
    "gatsby-remark-images": "^1.5.67",
    "gatsby-source-filesystem": "^1.5.39",
    "gatsby-transformer-remark": "^1.7.44",
    "gatsby-transformer-sharp": "^1.6.27",
    "lodash": "^4.17.10",
    "react-helmet": "^5.2.0"
  },
  "devDependencies": {
    "gh-pages": "^1.2.0"
  }
}
